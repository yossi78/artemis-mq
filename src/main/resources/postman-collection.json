{
  "info": {
    "name": "Artemis Queue API",
    "description": "Postman collection for CRUD operations on Artemis MQ",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Add User to Queue",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"firstName\": \"John\",\n  \"lastName\": \"Doe\"\n}"
        },
        "url": {
          "raw": "http://localhost:8080/users",
          "protocol": "http",
          "host": ["localhost"],
          "port": "8080",
          "path": ["users"]
        }
      },
      "response": []
    },
    {
      "name": "Consume User from Queue",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "http://localhost:8080/users",
          "protocol": "http",
          "host": ["localhost"],
          "port": "8080",
          "path": ["users"]
        }
      },
      "response": []
    }
  ]
}
